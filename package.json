    "jest-cli": "^0.4.5",
    "webpack": "^1.9.9"
  },
  "peerDependencies": {
    "react": "^0.14.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
